<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <author>{{author}}</author>
    <title>{{title}}</title>
    {%for item in items %}
    <entry>
        <title>{{item.title}}</title>
        <link href="{{item.link}}"/>
        <content type="html">{{item.content}}</content>
        <id>{{item.id}}</id>
    </entry>
    {%endfor%}
</feed>
